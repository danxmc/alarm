{
  "name": "alarmsys",
  "version": "1.0.0",
  "description": "MERN Stack System for activating/deactivating alarm",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "herouku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "seed": "md-seed run --dropdb"
  },
  "author": "danx",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "config": "^3.0.1",
    "express": "^4.16.4",
    "johnny-five": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.20",
    "mongoose-data-seed": "^1.0.7"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
